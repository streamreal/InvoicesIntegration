        <DTS:Executable
          DTS:refId="Package\Система уведомлений\INSERT решение по товарам"
          DTS:CreationName="Microsoft.ExecuteSQLTask"
          DTS:Description="Задача &quot;Выполнение SQL&quot;"
          DTS:DTSID="{00BAB4E6-7EFB-4FAE-BF64-D3EF0372C214}"
