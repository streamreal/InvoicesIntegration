              <properties>
                <property
                  dataType="System.String"
                  description="Specifies the SQL statement used by the component to extract data."
                  expressionType="Notify"
                  name="SqlCommand"
                  UITypeEditor="Microsoft.DataTransformationServices.Controls.ModalMultilineStringEditor">WITH cte_zero AS (
	SELECT 
	dc.*, 
	workers.* 
	,0 [Кол-во доставок]
	,0 [Кол-во грузов]
	,0 [Кол-во плеч по перевозке (подрядчиков)]
	,0 [Кол-во контейнеров]
	,0 [Кол-во ТС]
	,0 [Кол-во грузов с ГО]
	,0 [Кол-во доставок с ДТ с условиями группы Е]	
	,0 [Кол-во доставок с ДТ с условиями группы F]	
	,0 [Кол-во доставок с ДТ с условиями группы C]	
	,0 [Кол-во доставок с ДТ с условиями группы D]	
	,0 [Кол-во доставок с новыми заказчиками/видами перевозки]	
	FROM 
	(SELECT DISTINCT id_month, id_year 
	FROM [BI].[dbo].[DimCalendar]
	WHERE id_year &gt;= 2020
	AND (id_year &lt; YEAR(GETDATE()) OR (id_year = YEAR(GETDATE()) AND id_month &lt;= MONTH(GETDATE())))
	) dc
	CROSS JOIN 
	(SELECT DISTINCT [department], [name], [code] 
	FROM [motivation].[dbo].[stl_workers]) workers
	),
	
	cte_datamart AS (
	SELECT	
		MONTH(DRLLastFactDate) month_id
		,YEAR(DRLLastFactDate) year_id
		,[Exec_ Department Code] Отдел
		,SalespersonName Специалист
		,[Executive Code] Код
		,COUNT(DISTINCT dd.[No_]) [Кол-во доставок]
		,SUM(COALESCE(CargoQty, 0)) [Кол-во грузов]
		,SUM(COALESCE(VendorQty, 0)) [Кол-во плеч по перевозке (подрядчиков)]
		,SUM(COALESCE(ContainerQty, 0)) [Кол-во контейнеров]
		,SUM(COALESCE(Transport_Qty, 0)) [Кол-во ТС]
		,SUM(COALESCE(CargoID_Ins_Qty, 0)) [Кол-во грузов с ГО]			
		
		,SUM(CASE LEFT(gtd.terms, 1) WHEN 'E' THEN 1 ELSE 0 END) [Кол-во доставок с ДТ с условиями группы Е]	
		,SUM(CASE LEFT(gtd.terms, 1) WHEN 'F' THEN 1 ELSE 0 END) [Кол-во доставок с ДТ с условиями группы F]	
		,SUM(CASE LEFT(gtd.terms, 1) WHEN 'C' THEN 1 ELSE 0 END) [Кол-во доставок с ДТ с условиями группы C]	
		,SUM(CASE LEFT(gtd.terms, 1) WHEN 'D' THEN 1 ELSE 0 END) [Кол-во доставок с ДТ с условиями группы D]	
			
		,SUM(CASE COALESCE(esd.rn, 0) WHEN 1 THEN 1 ELSE 0 END) [Кол-во доставок с новыми заказчиками/видами перевозки]	
	
	FROM [BI].[dwh].[DeliveryDatamart] dd
	JOIN [BI].[dwh].[ETP$Salesperson_Purchaser] sp ON sp.SalespersonCode = dd.[Executive Code]
	
	LEFT JOIN (SELECT UPPER(LTRIM([20_20])) terms, UPPER(LTRIM(RTRIM([&amp;&amp;PRIM]))) delivery, 
		ROW_NUMBER() OVER (PARTITION BY [&amp;&amp;PRIM] ORDER BY (SELECT NULL)) rn
		FROM [gtd].[dbo].[GTDList]
		WHERE [RegNum] IS NOT NULL AND [RegNum] &lt;&gt; '') gtd
	ON gtd.delivery = dd.No_ AND gtd.rn = 1	
	
	LEFT JOIN (SELECT [No_], ROW_NUMBER() 
							OVER (PARTITION BY [Customer No_], [Transport Method] 
							ORDER BY [Document Date] ASC) rn
				FROM [BI].[dwh].[ETP$SQL_Delivery]) esd
	ON esd.No_ = dd.No_ AND esd.rn = 1

	WHERE DRLLastFactDate &gt;= '20200101'
	GROUP BY 
	MONTH(DRLLastFactDate), 
	YEAR(DRLLastFactDate), 
	[Exec_ Department Code], 
	SalespersonName,
	[Executive Code]
	),

	cte_datamart_plus_zero AS (
	SELECT * FROM cte_datamart 
	UNION ALL
	SELECT * FROM cte_zero AS z
	WHERE NOT EXISTS (SELECT 0 FROM cte_datamart AS d 
						WHERE d.[Код] = z.code AND d.month_id = z.id_month AND d.year_id = z.id_year)	
	),

	cte AS (
	SELECT 
	c.*
	,CASE
		WHEN EXISTS (SELECT 0 FROM [motivation].[dbo].[stl_workers] w
					 WHERE w.code = [Код]
					 AND w.is_archive = 0
					 AND w.estimate_participant = 1
					 AND w.date_begin IS NULL
					 AND w.date_end IS NULL) 
		THEN 1
		WHEN EXISTS (SELECT 0 FROM [motivation].[dbo].[stl_workers] w
					 WHERE w.code = [Код]
					 AND w.is_archive = 1
					 AND w.estimate_participant = 1
					 AND CAST(DATEFROMPARTS(year_id, month_id, 15) AS date) 
						 BETWEEN w.date_begin AND w.date_end) 
		THEN 1
		ELSE 0
		END [Участвует в расчете (1)]
	
	FROM cte_datamart_plus_zero AS c
	),

	cte2 AS (
	SELECT 
	c.*

	,(SELECT COUNT(0)
		FROM [BI].[etl].[timesheet] ts 
		WHERE 
		MONTH(ts.[Date]) = c.month_id
		AND YEAR(ts.[Date]) = c.year_id
		AND ts.[Date] &lt;= GETDATE()
		AND ts.Letter_code = 'Я'
		AND ts.Person = c.[Специалист]) [Количество отработанных дней]	

	,(SELECT COUNT(0) FROM [BI].[dbo].[DimCalendar] dc
	WHERE dc.id_year = c.year_id
	AND dc.id_month = c.month_id
	AND dc.WorkDay = 1) [Количество рабочих дней в месяце]		   
	
	,COUNT(CASE WHEN [Участвует в расчете (1)] = 1 THEN 1 END) OVER (PARTITION BY [Отдел], year_id, month_id) [Количество сотрудников участвующих в расчете по отделу]

	,f.stl_delivery [За доставку]
	,f.stl_cargo [За груз]
	,f.stl_contractor [За плечо]
	,f.stl_container [За контейнер]
	,f.stl_vehicle [За ТС]
	,f.stl_go [За груз с ГО]

	FROM cte c
	JOIN [motivation].[dbo].[stl_factors] f ON f.id = 1
	),

	cte3 AS (
	SELECT 	
	c.* 
	
	,COALESCE(
		CAST((1.00  * c.[Количество отработанных дней] / 	
		NULLIF(c.[Количество рабочих дней в месяце], 0)) 
	AS decimal(18,2))
	, 0) [Коэффициент рабочих дней]
	
	,CAST(
	(c.[За доставку]       * c.[Кол-во доставок]  
	+ c.[За груз]       * c.[Кол-во грузов]  
	+ c.[За плечо]		* c.[Кол-во плеч по перевозке (подрядчиков)]
	+ c.[За контейнер]	* c.[Кол-во контейнеров]
	+ c.[За ТС]			* c.[Кол-во ТС]
	+ c.[За груз с ГО]	* c.[Кол-во грузов с ГО]) AS decimal(18,2)) [Оценка сотрудника]

	FROM cte2 c
	),

	cte4 AS (
	SELECT c.* 

	,COALESCE(
		CAST(
		(1.00 * c.[Оценка сотрудника] / NULLIF(c.[Коэффициент рабочих дней], 0))
		AS decimal(18,2))
	, 0) [Оценка сотрудника с поправкой на количество отработанных дней]
	
	FROM cte3 c
	),

	cte5 AS(	
	SELECT 	
	c.* 
	
	,
	CAST(
	AVG(c.[Оценка сотрудника с поправкой на количество отработанных дней])
	OVER (PARTITION BY c.Отдел) 
	AS decimal(18, 2))
	[Средняя оценка по группе]
	
	,
	CAST(
		c.[Оценка сотрудника с поправкой на количество отработанных дней]
		/
		COALESCE(
			NULLIF(
				AVG(c.[Оценка сотрудника с поправкой на количество отработанных дней])
				OVER (PARTITION BY c.Отдел)  
			, 0)
		, 0)
	AS decimal(18, 2))
	[Коэффициент Сотрудника]	

	FROM cte4 c
	)
	
	SELECT * FROM cte5</property>
