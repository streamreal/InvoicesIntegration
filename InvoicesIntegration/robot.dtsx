    {
        
        base.PreExecute();

        string query = @"SELECT 
       COALESCE([article], '')
      ,COALESCE([model], '')
      ,COALESCE([manufacturer], '')
      ,COALESCE([trade_mark], '')
      ,COALESCE([country], '')
